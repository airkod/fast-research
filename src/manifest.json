{
  "manifest_version": 3,
  "name": "Fast Research",
  "version": "1.0",
  "description": "Fast Research",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_title": "Click to Fast Research",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "assets/icon.png",
    "32": "assets/icon.png",
    "48": "assets/icon.png",
    "128": "assets/icon.png"
  }
}
